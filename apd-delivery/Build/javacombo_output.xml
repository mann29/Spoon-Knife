<?xml version="1.0" encoding="utf-8" ?>
<!--This file contains the list of all binary dependencies that need to be copied before compiling projects/solution.
    Currently, the following keyword for build locations are supported:
    $(SourceDir), $(BuildDir), $(DropLocation)
-->
<CopyFilesWildcard>
    <CopyFiles>
    <SourcePath>$(SourceDir)\source\apd-delivery-svc\ear\target</SourcePath>
    <DestinationPath>$(DropLocation)\SCM_Apd-Delivery\Img\java</DestinationPath>
    <FilePatterns>*.ear</FilePatterns>
  </CopyFiles>
  <!-- ========= This is the standard Maven copy block for config , sql and other directories ========= -->
  <CopyFiles>
    <SourcePath>$(SourceDir)\sql</SourcePath>
    <DestinationPath>$(DropLocation)\SCM_Apd-Delivery\Img\sql</DestinationPath>
    <FilePatterns>*.*</FilePatterns>
  </CopyFiles>
  <CopyFiles>
    <SourcePath>$(SourceDir)\config</SourcePath>
    <DestinationPath>$(DropLocation)\SCM_Apd-Delivery\Img\config</DestinationPath>
    <FilePatterns>*.*</FilePatterns>
  </CopyFiles>
  <CopyFiles>
    <SourcePath>$(SourceDir)\documents\deploy</SourcePath>
    <DestinationPath>$(DropLocation)\SCM_Apd-Delivery\documents\deploy</DestinationPath>
    <FilePatterns>*.*</FilePatterns>
  </CopyFiles>
  <CopyFiles>
    <SourcePath>$(SourceDir)\documents\release-notes</SourcePath>
    <DestinationPath>$(DropLocation)\SCM_Apd-Delivery\documents\release-notes</DestinationPath>
    <FilePatterns>*.*</FilePatterns>
  </CopyFiles>
  
  <!-- =================================================================================================-->
</CopyFilesWildcard>
